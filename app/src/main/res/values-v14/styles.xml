<?xml version="1.0" encoding="utf-8"?>
<resources>

        <style name="profile_edittext">
                <item name="android:textColorHint">@color/gray_edittext</item>
                <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
                <item name="android:textSize">@dimen/dim_commontext_size</item>
                <item name="android:textAllCaps">false</item>
                <item name="android:textColor">@color/black</item>
                <item name="android:singleLine">true</item>
                <item name="android:gravity">center_vertical</item>
                <item name="android:paddingTop">20dp</item>
                <item name="android:paddingBottom">20dp</item>
                <item name="android:paddingRight">@dimen/padding_left</item>
                <item name="android:paddingLeft">@dimen/padding_left</item>
                <item name="android:layout_marginTop">0dp</item>
                <item name="android:layout_marginBottom">0dp</item>
                <item name="android:layout_marginRight">@dimen/padding_left</item>
                <item name="android:layout_marginLeft">@dimen/padding_left</item>
        </style>
</resources>